<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nice.mapper.UserAMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.nice.domain.UserA">
		<id column="id" property="id" />
		<result column="userNOA" property="userNo" />
		<result column="userNOB" property="password" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, userNo AS userNo, password,  remark
    </sql>

</mapper>
